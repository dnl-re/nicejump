<idea-plugin>
  <id>re.dnl.nicejump</id>
  <name>YAJP - Yet Another Jump Plugin</name>
  <version>0.1</version>

  <description><![CDATA[
      Jump, select or delete until specified position.
    ]]></description>
  <vendor>JetBrains</vendor>

  <change-notes><![CDATA[
        <html>
        <dl>
            <dt>1.0</dt>
            <dd>Initial Release</dd>
        </dl>
        </html>
    ]]>
  </change-notes>
  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="145.0"/>

  <depends>com.intellij.modules.lang</depends>

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
  </extensions>

  <actions>
    <!-- Add your actions here -->
    <action id="MoveAction"
            class="niceJump.src.re.dnl.nicejump.MoveAction"
            text="Nice Jump"
            description="Search and navigate to result">
      <keyboard-shortcut keymap="$default"
                         first-keystroke="ctrl COMMA"/>
    </action>

  </actions>
</idea-plugin>